{
  "name": "next-ttag-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "trans-update": "npx ttag update i18n/uk.po src/",
    "trans-create": "npx ttag extract --output i18n/uk.po ./src",
    "po-to-json": "ttag po2json i18n/uk.po > i18n/uk.po.json"
  },
  "dependencies": {
    "next": "14.0.3",
    "react": "^18",
    "react-dom": "^18",
    "ttag": "^1.8.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "ttag-cli": "^1.10.9",
    "typescript": "^5"
  }
}
